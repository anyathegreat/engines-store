{
  "$schema": "https://json.schemastore.org/prettierrc",
  "semi": true,
  "tabWidth": 2,
  "singleQuote": false,
  "printWidth": 120,
  "trailingComma": "all",
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "git.autofetch": true,
  "eslintIntegration": true,
  "importOrder": ["^@/components/(.*)$", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
